<%= form_with url: merchant_bulk_discounts_path(@merchant), method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :percentage %>
  <%= f.text_field :percentage %>
  <%= f.label :quantity_threshold %>
  <%= f.text_field :quantity_threshold %>
  <%= f.submit "Submit" %>
<% end %>