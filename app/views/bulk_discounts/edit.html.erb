<%= form_with model: @bulk_discount, url: merchant_bulk_discount_path(@merchant, @bulk_discount), method: :patch, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :percentage %>
  <%= f.text_field :percentage %>
  <%= f.label :quantity_threshold %>
  <%= f.text_field :quantity_threshold %>
  <%= f.submit "Submit" %>
<% end %>